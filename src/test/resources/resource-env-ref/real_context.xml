              maxIdle="10" name="jdbc/Sybase"
              password="..." removeAbandonedTimeout="30"
              type="javax.sql.DataSource"
              url="jdbc:sybase:Tds:...:5000?DYNAMIC_PREPARE=true&#038;APPLICATIONNAME=opixweb"
              username="..." validationQuery="select 1"/>
